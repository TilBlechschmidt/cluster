{
  "name": "cdk8s",
  "version": "1.0.0",
  "main": "ts/main.js",
  "types": "src/main.ts",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "import": "cdk8s import --output src/imports",
    "synth": "cdk8s synth -a 'yarn ts-node src/main.ts'",
    "compile": "tsc --build",
    "watch": "tsc --build -w",
    "build": "yarn compile && yarn synth",
    "upgrade": "yarn i cdk8s@latest cdk8s-cli@latest",
    "upgrade:next": "yarn i cdk8s@next cdk8s-cli@next"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^14.18.43",
    "@types/pbkdf2": "^3.1.0",
    "@types/bcrypt": "^5.0.0",
    "bcrypt": "^5.1.0",
    "cdk8s": "^2.7.56",
    "cdk8s-cli": "^2.2.29",
    "cdk8s-plus-26": "^2.3.17",
    "constructs": "^10.2.15",
    "js-yaml": "^4.1.0",
    "pbkdf2": "^3.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
